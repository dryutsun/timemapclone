<form action="/events/new" method="POST">
    <div>  
      <label for="title">Title</label>
      <br>
      <input type="text" id="title" name="title" required> </div>

    <div>  
        <label for="date">Date: </label>
        <br>
        <small>Please enter Date in YYYY-MM-DD format.</small>
        <br>
        <input type="text" id="date" name="date" required>  </div>

      <div>  
        <label for="timeStart">Time Start</label>
        <br>
        <input type="text" id="timeStart" name="timeStart" required>
      </div>

      <div>  
        <label for="locationLon">Location Longitude</label>
        <br>
        <input type="text" id="locationLon" name="locationLon" required>
      </div>

      <div>  
        <label for="locationLat">Location Latitude</label>
        <br>
        <input type="text" id="locationLat" name="locationLat" required>
      </div>

      <div>  
        <label for="association">Association:</label>
        <br>
        <small> Please enter a descriptive tag.</small>
        <br>
        <input type="text" id="association" name="association" required>
      </div>

      <div>  
        <label for="sourcedata">Source Data: </label>
        <br>
        <input type="text" id="sourcedata" name="sourcedata" required>
      </div>

      <div>  
        <label for="comments">Comments:</label>
        <br>
        <input type="text" id="comments" name="comments" required>     
     </div>
   
     <div>
         <label for="projectId">Project Name:</label>
         <br>
        <select name="projectId" required>
            <% var options = []; %>
            <% project.forEach((project, index) => { %>
                <option value='<%= project.id %>'><%= project.title %></option>
            <% }) %>
        </select>
    </div>
      <button type="submit" class="btn btn-primary">Submit</button>
  </form>
  
  <div class="map-container" styles="display:flex; flex-direction: column; align-items: center;">
    <div id='map' style="height: 400px; width: 400px; position: relative;">
    </div>
</div>

    <script src="/static/js/map.js">
    </script>