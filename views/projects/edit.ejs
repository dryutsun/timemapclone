
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
<link rel="stylesheet" href="/static/css/project-grid.css">


<div class="container d-flex project-wrapper">
<form action="/projects/edit/<%=project.id %>/?_method=PUT" method="POST">
    
      <div class="field-item form-control-lg">  
        <label for="title">Title</label>
        <br>
        <input type="text"  id="title" name="title" value="<%= project.title %>">
      </div>
  
      <div class="field-item form-control-lg">  
          <label for="description">Description: </label>
          <div id="project-description" class="form-text small">Please enter in a small blurb about your investigation.</div>
          <input type="text" id="description" name="description" value="<%= project.description %>">
      </div>
  
      <button type="submit" class="btn btn-primary">Submit</button>

</form>
</div>

