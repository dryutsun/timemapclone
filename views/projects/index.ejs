<div class="text-center container">
<h2><%= currentUser.name %>'s Profile</h2>



<a class="btn btn-primary" href="/projects/new" role="button">Add A New Project</a>
<h1> Projects: </h1>
<div class="project-grid">
<% project.forEach((project)=>{%>
    <div class="card" style="outline: red dotted 1px; background-color:azure; width: 100%; height: 100%;">
        <a href="/projects/<%= project.id %>"><h1> <%= project.title %></h1></a>
        <h3> <%= project.description %></h3>
        <small><%= project.createdAt%></small>
        <small><%= project.updatedAt%></small>

    </div>
<%})%>
</div>

<link rel="stylesheet" href="/static/css/project-grid.css">
</div>